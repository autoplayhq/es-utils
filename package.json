    "tslib": "^2.4",
    "zod": "^3"
  },
  "devDependencies": {
    "@jest/globals": "^28.1.3",
    "@swc/core": "^1.2.220",
    "@swc/jest": "^0.2.22",
    "jest": "^28.1.3",
    "rimraf": "^3.0.2",
    "tslib": "^2.4",
    "typescript": "^4.8.4",
    "zod": "^3.19.1"
  }
