    "prepare": "npm run clean && npm run build",
    "prepack": "npm run prepare"
  },
  "files": [
    "esm/*",
    "cjs/*",
    "src/*"
  ],
  "author": "Cole Lawrence <cole@colelawrence.com>",
  "repository": {
    "type": "git",
