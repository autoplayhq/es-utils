{
  "name": "@autoplay/utils",
  "version": "0.0.20",
  "description": "Utilities designed for error management, parsing, and TypeScript domain modeling.",
  "keywords": [],
  "type": "module",
  "exports": {
    "default": "./esm/index.js",
    "import": "./esm/index.js",
    "require": "./cjs/index.js"
  },
  "types": "esm/index.d.ts",
  "scripts": {
    "build": "tsc --build . && tsc --build tsconfig.cjs.json",
